{
    "name": "gitbook-plugin-sharing",
    "description": "Sharing buttons in GitBooks website",
    "main": "index.js",
    "version": "1.0.0",
    "dependencies": {
        "lodash": "^3.10.1"
    },
    "engines": {
        "gitbook": ">=2.4.0"
    },
    "homepage": "https://github.com/GitbookIO/plugin-sharing",
    "repository": {
        "type": "git",
        "url": "https://github.com/GitbookIO/plugin-sharing.git"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/GitbookIO/plugin-sharing/issues"
    },
    "gitbook": {
        "config": [
            {
                "key": "facebook",
                "type": "bool",
                "default": true,
                "description": "Facebook"
            },
            {
                "key": "twitter",
                "type": "bool",
                "default": true,
                "description": "Twitter"
            },
            {
                "key": "google",
                "type": "bool",
                "default": true,
                "description": "Google"
            },
            {
                "key": "weibo",
                "type": "bool",
                "default": false,
                "description": "Weibo"
            },
            {
                "key": "instapaper",
                "type": "bool",
                "default": false,
                "description": "Instapaper"
            },
            {
                "key": "vk",
                "type": "bool",
                "default": false,
                "description": "VK"
            }
        ]
    }
}
